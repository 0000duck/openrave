<?xml version="1.0" encoding="utf-8"?>

<!-- Template
    <Body name="" type="">
	  <Translation></Translation>
	  <RotationAxis></RotationAxis>
      <Geom type="">
        <Data>=</Data>
        <Render></Render>
      </Geom>
      <mass type="box">
        <mass></mass>
        <extents></extents>
      </mass>
    </Body>
-->

<Robot>
  <KinBody name="Lynx6">
    <Mass type="mimicgeom">
      <density>800</density>
    </Mass>

    <Body name="Link0" type="dynamic">
      <Mass type="mimicgeom">
        <density>16000</density>
      </Mass>
      <Geom type="cylinder">
        <Translation>0 0.0238 0</Translation>
        <radius>0.0492</radius>
        <height>0.0476</height>
        <diffuseColor>1 1 0</diffuseColor>
      </Geom>
      <Geom type="cylinder">
        <Translation>0 0.05315 0</Translation>
        <radius>0.015</radius>
        <height>0.0111</height>
        <diffuseColor>0.05 0.05 0.05</diffuseColor>
      </Geom>
    </Body>

	<Body name="Link1" type="dynamic">
	  <offsetfrom>Link0</offsetfrom>
      <Translation>0 0.06032 0</Translation>
      <Geom type="cylinder">
        <radius>0.046</radius>
        <height>0.003175</height>
        <diffuseColor>1 1 0</diffuseColor>
      </Geom>
    </Body>

    <Joint type="hinge">
      <Body>Link0</Body>
      <Body>Link1</Body>
      <offsetfrom>Link1</offsetfrom>
      <weight>4</weight>
      <lostop>-85</lostop>
      <histop>85</histop>
      <axis>0 1 0</axis>
      <maxvel>2</maxvel>
      <resolution>1</resolution>
    </Joint>

    <Body name="Link2" type="dynamic">
	  <offsetfrom>Link1</offsetfrom>
      <Translation>0 0.01349 0</Translation>
      <Geom type="box">
        <Translation>0.0 0.06109 0</Translation>
        <Extents>0.0119 0.073 0.02659</Extents>
        <diffuseColor>1 1 0</diffuseColor>
      </Geom>
      <Mass type="mimicgeom">
        <density>200</density>
      </Mass>
    </Body>

    <Joint type="hinge">
      <Body>Link1</Body>
      <Body>Link2</Body>
      <offsetfrom>Link2</offsetfrom>
      <weight>3</weight>
      <lostop>-80</lostop>
      <histop>80</histop>
      <axis>0 0 -1</axis>
      <maxvel>2</maxvel>
      <resolution>1</resolution>
    </Joint>

    <Body name="Link3" type="dynamic">
	  <offsetfrom>Link2</offsetfrom>
      <Translation>0 0.1222 0</Translation>
      <Geom type="box">
        <Translation>0.05238 0 0</Translation>
        <Extents>0.0921 0.0095 0.0127</Extents>
        <diffuseColor>1 1 0</diffuseColor>
      </Geom>
    </Body>

    <Joint type="hinge">
      <Body>Link2</Body>
      <Body>Link3</Body>
      <offsetfrom>Link3</offsetfrom>
      <weight>3</weight>
      <lostop>-60</lostop>
      <histop>80</histop>
      <axis>0 0 1</axis>
      <maxvel>2</maxvel>
      <resolution>1</resolution>
    </Joint>

    <Body name="Link4" type="dynamic">
	  <offsetfrom>Link3</offsetfrom>
      <Translation>0.12 0 0</Translation>
      <Geom type="box">
        <Translation>0.0166 0 0.02381</Translation>
        <Extents>0.0286 0.0127 0.00159</Extents>
        <diffuseColor>1 1 0</diffuseColor>
      </Geom>
      <Geom type="box">
        <Translation>0.0166 0 -0.02381</Translation>
        <Extents>0.0286 0.0127 0.00159</Extents>
        <diffuseColor>1 1 0</diffuseColor>
      </Geom>
      <Geom type="box">
        <Translation>0.0381 0 0</Translation>
        <Extents>0.0127 0.01 0.0254</Extents>
        <diffuseColor>1 1 0</diffuseColor>
      </Geom>
    </Body>

    <Joint type="hinge">
      <Body>Link3</Body>
      <Body>Link4</Body>
      <offsetfrom>Link4</offsetfrom>
      <weight>3</weight>
      <lostop>-100</lostop>
      <histop>100</histop>
      <axis>0 0 -1</axis>
      <maxvel>2</maxvel>
      <resolution>2</resolution>
    </Joint>

    <Body name="Link5" type="dynamic">
	  <offsetfrom>Link4</offsetfrom>
      <Translation>0.05915 0 -0.00635</Translation>
      <Geom type="box">
        <Translation>0.0127 -0.0127 0.0095</Translation>
        <Extents>0.018 0.007 0.0254</Extents>
      </Geom>
    </Body>

    <Joint type="hinge">
      <Body>Link4</Body>
      <Body>Link5</Body>
      <offsetfrom>Link5</offsetfrom>
      <weight>3</weight>
      <lostop>-85</lostop>
      <histop>85</histop>
      <axis>1 0 0</axis>
      <maxvel>2</maxvel>
      <resolution>4</resolution>
    </Joint>

    <!-- Manipulator -->

    <!-- Active Parts -->
    <Body name="ManipRD" type="dynamic">
	  <offsetfrom>Link5</offsetfrom>
      <RotationAxis>0 1 0 -45</RotationAxis>
      <Translation>0.017462 0.0016 0.0111</Translation>
      <Geom type="box">
        <Translation>0.0143 -0.0015 0</Translation>
        <Extents>0.0143 0.005 0.003175</Extents>
        <diffuseColor>0.05 0.05 0.05</diffuseColor>
      </Geom>
	  <!--
      <Mass type="mimicgeom">
        <density>38000</density>
      </Mass> -->
    </Body>
    <Body name="ManipLD" type="dynamic">
	  <offsetfrom>Link5</offsetfrom>
      <Translation>0.017462 0.0016 -0.0111</Translation>
      <RotationAxis>0 1 0 45</RotationAxis>
      <Geom type="box">
        <Translation>0.0143 -0.0015 0</Translation>
        <Extents>0.0143 0.005 0.003175</Extents>
        <diffuseColor>0.05 0.05 0.05</diffuseColor>
      </Geom>
	  <!--
      <Mass type="mimicgeom">
        <density>38000</density>
      </Mass>
	  -->
    </Body>

    <Joint name="JR" type="hinge">
      <Body>Link5</Body>
      <Body>ManipRD</Body>
      <offsetfrom>ManipRD</offsetfrom>
      <weight>3</weight>
      <lostop>-60</lostop>
      <histop>60</histop>
      <axis>0 1 0</axis>
      <maxvel>2</maxvel>
      <resolution>2</resolution>
    </Joint>
    <Joint name="JL" type="hinge">
      <Body>Link5</Body>
      <Body>ManipLD</Body>
      <offsetfrom>ManipLD</offsetfrom>
      <weight>3</weight>
      <lostop>-60</lostop>
      <histop>60</histop>
      <axis>0 -1 0</axis>
      <maxvel>2</maxvel>
      <resolution>2</resolution>
    </Joint>

    <!-- Passive Parts -->
    <Body name="ManipRU" type="dynamic">
	  <offsetfrom>Link5</offsetfrom>
      <Translation>0.03175 0.0016 0.00238</Translation>
      <RotationAxis>0 1 0 -45</RotationAxis>
      <Geom type="box">
        <Translation>0.0143 -0.0015 0</Translation>
        <Extents>0.0143 0.005 0.003175</Extents>
        <diffuseColor>0.05 0.05 0.05</diffuseColor>
      </Geom>
    </Body>
    <Body name="ManipLU" type="dynamic">
	  <offsetfrom>Link5</offsetfrom>
      <Translation>0.03175 0.0016 -0.00238</Translation>
      <RotationAxis>0 1 0 45</RotationAxis>
      <Geom type="box">
        <Translation>0.0143 -0.0015 0</Translation>
        <Extents>0.0143 0.005 0.003175</Extents>
        <diffuseColor>0.05 0.05 0.05</diffuseColor>
      </Geom>
    </Body>

    <Joint type="hinge" enable="false" mimic="JR">
      <Body>Link5</Body>
      <Body>ManipRU</Body>
      <offsetfrom>ManipRU</offsetfrom>
      <weight>3</weight>
      <lostop>-50</lostop>
      <histop>50</histop>
      <axis>0 1 0</axis>
    </Joint>
    <Joint type="hinge" enable="false" mimic="JL">
      <Body>Link5</Body>
      <Body>ManipLU</Body>
      <offsetfrom>ManipLU</offsetfrom>
      <weight>3</weight>
      <lostop>-50</lostop>
      <histop>50</histop>
      <axis>0 -1 0</axis>
    </Joint>

    <!-- form the manipulator links and join to the 2 support links on each side -->
    <Body name="ManipR" type="dynamic">
	  <offsetfrom>Link5</offsetfrom>
      <Translation>0.05397 0 0.01666</Translation>
      <Geom type="box">
        <Translation>0.0143 0 0</Translation>
        <Extents>0.0143 0.004 0.0032</Extents>
        <diffuseColor>0.05 0.05 0.05</diffuseColor>
      </Geom>
      <Geom type="box">
        <RotationAxis>0 1 0 31.4</RotationAxis>
        <Translation>-0.012 0.0021 0.005</Translation>
        <Extents>0.012 0.002 0.002</Extents>
        <diffuseColor>0.05 0.05 0.05</diffuseColor>
      </Geom>
      <!--<Mass type="mimicgeom">
        <density>18000</density>
      </Mass>-->
    </Body>
    <Body name="ManipL" type="dynamic">
	  <offsetfrom>Link5</offsetfrom>
      <Translation>0.05397 0 -0.01666</Translation>
      <Geom type="box">
        <Translation>0.0143 0 0</Translation>
        <Extents>0.0143 0.004 0.0032</Extents>
        <diffuseColor>0.05 0.05 0.05</diffuseColor>
      </Geom>
      <Geom type="box">
        <RotationAxis>0 1 0 -31.4</RotationAxis>
        <Translation>-0.012 0.0021 -0.005</Translation>
        <Extents>0.012 0.002 0.002</Extents>
        <diffuseColor>0.05 0.05 0.05</diffuseColor>
      </Geom>
      <!--<Mass type="mimicgeom">
        <density>18000</density>
      </Mass>-->
    </Body>

    <Joint type="hinge" enable="false" mimic="JL">
      <Body>ManipLD</Body>
      <Body>ManipL</Body>
      <offsetfrom>ManipLD</offsetfrom>
      <weight>3</weight>
      <lostop>-90</lostop>
      <histop>90</histop>
      <axis>0 1 0</axis>
      <anchor>0.0143 0 0</anchor>
    </Joint>
    <Joint type="hinge" enable="false" mimic="JL">
      <Body>ManipLU</Body>
      <Body>ManipL</Body>
      <offsetfrom>ManipLU</offsetfrom>
      <weight>3</weight>
      <lostop>-90</lostop>
      <histop>90</histop>
      <axis>0 1 0</axis>
      <anchor>0.0143 0 0</anchor>
    </Joint>
    <Joint type="hinge" enable="false" mimic="JR">
      <Body>ManipRD</Body>
      <Body>ManipR</Body>
      <offsetfrom>ManipRD</offsetfrom>
      <weight>3</weight>
      <lostop>-90</lostop>
      <histop>90</histop>
      <axis>0 -1 0</axis>
      <anchor>0.0143 0 0</anchor>
    </Joint>
    <Joint type="hinge" enable="false" mimic="JR">
      <Body>ManipRU</Body>
      <Body>ManipR</Body>
      <offsetfrom>ManipRU</offsetfrom>
      <weight>3</weight>
      <lostop>-90</lostop>
      <histop>90</histop>
      <axis>0 -1 0</axis>
      <anchor>0.0143 0 0</anchor>
    </Joint>
  </KinBody>

  <!-- Items needs to be declared after KinBody -->

  <Manipulator>
    <effector>Link5</effector>
	<base>Link0</base>
    <joints>JR JL</joints>
    <!-- joint values of the closed and opened positions -->
    <closingdirection>-1.0 -1.0</closingdirection>
    <palmdirection>1 0 0</palmdirection>
    <!-- grasp goal with respect to the effector -->
    <Translation>0.08 0 0</Translation>
  </Manipulator>
  <trackingTolerance>0.1</trackingTolerance>
  <goalTolerance>0.01</goalTolerance>
</Robot>
