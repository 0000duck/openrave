<!-- Manus Arm from Exact Dynamics BV -->
<Robot name="ManusArm-Left">
  <KinBody>
    <!-- leftarm -->
    <Body name="l-lift-arm" type="dynamic">
      <Geom type="trimesh">
        <Render>models/manusarm/l-lift-arm.wrl 0.001</Render>
        <data>models/manusarm/l-lift-arm.wrl 0.001</data>
      </Geom>
    </Body>
    <Body name="left-arm1" type="dynamic">
      <Translation>0.045416  -0.147592   0.467052</Translation>
      <Geom type="trimesh">
        <RotationAxis>0 1 0 180</RotationAxis>
        <Render>models/manusarm/arm1.wrl 0.001</Render>
        <data>models/manusarm/arm1.wrl 0.001</data>
      </Geom>
      <Geom type="cylinder">
        <radius>0.07</radius>
        <height>0.09</height>
        <translation>0 0.45 0</translation>
        <diffusecolor>0 0 0</diffusecolor>
        <diffusecolor>0.2 0.2 0.4</diffusecolor>
      </Geom>
    </Body>
    <Body name="left-arm2" type="dynamic">
      <Translation>-0.084734   0.302408   0.467052</Translation>
      <Geom type="trimesh">
        <RotationAxis>1 0 0 -90</RotationAxis>
        <RotationAxis>0 1 0 -90</RotationAxis>
        <RotationAxis>0 0 1 180</RotationAxis>
        <Render>models/manusarm/arm2.wrl 0.001</Render>
        <data>models/manusarm/arm2.wrl 0.001</data>
      </Geom>
    </Body>
    <Body name="left-arm3" type="dynamic">
      <Translation>-0.084734   0.302408   0.872052</Translation>
      <RotationAxis>0 1 0 -90</RotationAxis>
      <RotationAxis>0 0 1 90</RotationAxis>
      <RotationAxis>0 0 1 -180</RotationAxis>
      <Geom type="trimesh">
        <Render>models/manusarm/arm3.wrl 0.001</Render>
        <data>models/manusarm/arm3.wrl 0.001</data>
      </Geom>
    </Body>
    <Body name="left-arm4" type="dynamic">
      <Translation>-0.054734   0.302408   1.048052</Translation>
      <RotationAxis>1 0 0 90</RotationAxis>
      <Geom type="trimesh">
        <Render>models/manusarm/arm4.wrl 0.001</Render>
        <data>models/manusarm/arm4.wrl 0.001</data>
      </Geom>
    </Body>
    <Body name="left-arm5" type="dynamic">
      <Translation>-0.054734   0.302408   1.203052</Translation>
      <Geom type="trimesh">
        <RotationAxis>0 1 0 -90</RotationAxis>
        <Render>models/manusarm/arm5.wrl 0.001</Render>
        <data>models/manusarm/arm5.wrl 0.001</data>
      </Geom>
    </Body>
    <Body name="left-hand-base" type="dynamic">
      <Translation> -0.054734   0.302408   1.203052</Translation>
      <RotationAxis>1 0 0 90</RotationAxis>
      <Geom type="trimesh">
        <Render>models/manusarm/hand-base.wrl 0.001</Render>
        <data>models/manusarm/hand-base.wrl 0.001</data>
      </Geom>
    </Body>
    <Body name="left-hand-proximal-r" type="dynamic">
      <Translation>-0.0285686 0.0621171 0</Translation>
      <RotationAxis>0 0 1 90</RotationAxis>
      <RotationAxis>0 1 0 -90</RotationAxis>
      <offsetfrom>left-hand-base</offsetfrom>
      <Geom type="trimesh">
        <Render>models/manusarm/hand-proximal-b.wrl 0.001</Render>
        <data>models/manusarm/hand-proximal-b.wrl 0.001</data>
      </Geom>
    </Body>
    <Body name="left-hand-proximal-l" type="dynamic">
      <Translation>0.0285686 0.0621171 0</Translation>
      <RotationAxis>0 0 1 90</RotationAxis>
      <RotationAxis>0 1 0 90</RotationAxis>
      <offsetfrom>left-hand-base</offsetfrom>
      <Geom type="trimesh">
        <Render>models/manusarm/hand-proximal-b.wrl 0.001</Render>
        <data>models/manusarm/hand-proximal-b.wrl 0.001</data>
      </Geom>
    </Body>
    <Body name="left-hand-distal-r" type="dynamic">
      <Translation>-0.0285686 0.1261 0</Translation>
      <RotationAxis>0 0 1 90</RotationAxis>
      <RotationAxis>0 1 0 90</RotationAxis>
      <RotationAxis>0 0 1 -90</RotationAxis>
      <offsetfrom>left-hand-base</offsetfrom>
      <Geom type="trimesh">
        <Render>models/manusarm/hand-distal.wrl 0.001</Render>
        <data>models/manusarm/hand-distal.wrl 0.001</data>
      </Geom>
    </Body>
    <Body name="left-hand-distal-l" type="dynamic">
      <Translation>0.0285686 0.1261 0</Translation>
      <RotationAxis>0 0 1 90</RotationAxis>
      <RotationAxis>0 1 0 -90</RotationAxis>
      <RotationAxis>0 0 1 90</RotationAxis>
      <offsetfrom>left-hand-base</offsetfrom>
      <Geom type="trimesh">
        <Render>models/manusarm/hand-distal.wrl 0.001</Render>
        <data>models/manusarm/hand-distal.wrl 0.001</data>
      </Geom>
    </Body>

    <!-- left arm joints -->
    <Joint name="lJ0" type="hinge">
      <Body>l-lift-arm</Body>
      <Body>left-arm1</Body>
      <offsetfrom>left-arm1</offsetfrom>
      <weight>1</weight>
      <lostop>-179</lostop>
      <histop>179</histop>
      <axis>0 1 0</axis>
      <maxvel>0.5</maxvel>
      <resolution>1</resolution>
    </Joint>
    <Joint name="lJ1" type="hinge">
      <Body>left-arm1</Body>
      <Body>left-arm2</Body>
      <offsetfrom>left-arm2</offsetfrom>
      <weight>1</weight>
      <lostop>-179</lostop>
      <histop>179</histop>
      <axis>1 0 0</axis>
      <maxvel>0.5</maxvel>
      <resolution>1</resolution>
    </Joint>
    <Joint name="lJ2" type="hinge">
      <Body>left-arm2</Body>
      <Body>left-arm3</Body>
      <offsetfrom>left-arm3</offsetfrom>
      <weight>1</weight>
      <lostop>-179</lostop>
      <histop>179</histop>
      <axis>0 1 0</axis>
      <maxvel>0.5</maxvel>
      <resolution>1</resolution>
    </Joint>
    <Joint name="lJ3" type="hinge">
      <Body>left-arm3</Body>
      <Body>left-arm4</Body>
      <offsetfrom>left-arm4</offsetfrom>
      <weight>1</weight>
      <lostop>-179</lostop>
      <histop>179</histop>
      <axis>0 1 0</axis>
      <maxvel>0.5</maxvel>
      <resolution>1</resolution>
    </Joint>
    <Joint name="lJ4" type="hinge">
      <Body>left-arm4</Body>
      <Body>left-arm5</Body>
      <offsetfrom>left-arm5</offsetfrom>
      <weight>1</weight>
      <lostop>0</lostop>
      <histop>120</histop>
      <axis>0 -1 0</axis>
      <maxvel>0.5</maxvel>
      <resolution>1</resolution>
    </Joint>
    <Joint name="lJ5" type="hinge">
      <Body>left-arm5</Body>
      <Body>left-hand-base</Body>
      <offsetfrom>left-hand-base</offsetfrom>
      <weight>1</weight>
      <lostop>-179</lostop>
      <histop>179</histop>
      <axis>0 1 0</axis>
      <maxvel>0.5</maxvel>
      <resolution>1</resolution>
    </Joint>
    <Joint name="lJ-gripper" type="hinge">
      <Body>left-hand-base</Body>
      <Body>left-hand-proximal-r</Body>
      <offsetfrom>left-hand-proximal-r</offsetfrom>
      <weight>1</weight>
      <lostop>-45</lostop>
      <histop>6</histop>
      <axis>0 -1 0</axis>
      <maxvel>0.5</maxvel>
      <resolution>4</resolution>
    </Joint>
    <Joint type="hinge" enable="false" mimic="lJ-gripper 1 0">
      <Body>left-hand-proximal-r</Body>
      <Body>left-hand-distal-r</Body>
      <offsetfrom>left-hand-distal-r</offsetfrom>
      <weight>1</weight>
      <lostop>-179</lostop>
      <histop>179</histop>
      <axis>0 -1 0</axis>
      <maxvel>0.5</maxvel>
      <resolution>4</resolution>
    </Joint>
    <Joint type="hinge" enable="false" mimic="lJ-gripper 1 0">
      <Body>left-hand-base</Body>
      <Body>left-hand-proximal-l</Body>
      <offsetfrom>left-hand-proximal-l</offsetfrom>
      <weight>1</weight>
      <lostop>-179</lostop>
      <histop>179</histop>
      <axis>0 -1 0</axis>
      <maxvel>0.5</maxvel>
      <resolution>4</resolution>
    </Joint>
    <Joint type="hinge" enable="false" mimic="lJ-gripper 1 0">
      <Body>left-hand-proximal-l</Body>
      <Body>left-hand-distal-l</Body>
      <offsetfrom>left-hand-distal-l</offsetfrom>
      <weight>1</weight>
      <lostop>-179</lostop>
      <histop>179</histop>
      <axis>0 -1 0</axis>
      <maxvel>0.5</maxvel>
      <resolution>4</resolution>
    </Joint>
    <adjacent>l-lift-arm left-arm3</adjacent>
    <adjacent>left-hand-distal-r left-hand-distal-l</adjacent>
  </KinBody>
  <Manipulator name="leftarm">
    <base>l-lift-arm</base>
    <effector>left-hand-base</effector>
    <armjoints>lJ0 lJ1 lJ2 lJ3 lJ4 lJ5</armjoints>
    <joints>lJ-gripper</joints>
    <closingdirection>1</closingdirection>
    <palmdirection>0 1 0</palmdirection>
    <iksolver>ManusLeftArmikfast</iksolver>
  </Manipulator>
  
</Robot>
